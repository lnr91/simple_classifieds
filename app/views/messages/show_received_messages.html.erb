<table>
  <tr>
    <td>Classified Title</td>
    <td>From</td>
    <td>Message</td>
    <td>Reply</td>
  </tr>
  <% @messages.each do |message| %>
      <tr>
        <td><%= message.classified.name %></td>
        <td><%= message.sender.email%></td>
        <td><%= message.content %></td>
        <td>Reply</td>
      </tr>
  <% end %>


</table>